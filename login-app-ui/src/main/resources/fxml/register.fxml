<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane prefHeight="191.0" prefWidth="231.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.register.RegisterController">
    <children>
        <StackPane>
            <children>
                <VBox prefHeight="140.0" prefWidth="211.0" spacing="10.0">
                    <children>
                        <GridPane>
