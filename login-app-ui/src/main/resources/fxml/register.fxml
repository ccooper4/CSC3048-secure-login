                                <Label text="First name:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                                <Label text="Last name:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                                <TextField fx:id="fName" promptText="Enter first name" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                                <TextField fx:id="sName" promptText="Enter last name" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Label text="Email:" GridPane.rowIndex="2" />
                        <TextField fx:id="email" promptText="Enter email" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <TextField fx:id="password" promptText="*******" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <Label text="Password:" GridPane.rowIndex="3" />
                            </children>
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="65.0" />
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="146.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                        </GridPane>
                  <Pane prefHeight="36.0" prefWidth="211.0">
                     <children>
                        <Button id="registerButton" fx:id="registerButton" alignment="CENTER" contentDisplay="CENTER" defaultButton="true" layoutX="76.0" layoutY="-5.0" mnemonicParsing="false" text="Register" />
                     </children>
                  </Pane>
                    </children>
                </VBox>
            </children>
        </StackPane>
      <Pane prefHeight="202.0" prefWidth="211.0">
         <children>
            <Hyperlink fx:id="acceptableUsage" layoutX="38.0" layoutY="160.0" text="Acceptable usage policy" textFill="RED" underline="true" />
         </children>
      </Pane>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</StackPane>
