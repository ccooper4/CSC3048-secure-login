<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.control.Label?>
<StackPane id="pane" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controllers.RegistrationInfoController">
    <children>
        <StackPane>
            <children>
                <VBox spacing="20.0">
                    <children>

                        <!-- Button Label-->
                        <FlowPane>
                            <Label fx:id="loginID">
                            </Label>
                        </FlowPane>

                        <!-- Proceed Button-->
                        <FlowPane alignment="CENTER">
                            <Button fx:id="proceedButton" text="Proceed" onAction="#proceed">
                            </Button>
                        </FlowPane>

                    </children>
                </VBox>
            </children>
        </StackPane>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</StackPane>
